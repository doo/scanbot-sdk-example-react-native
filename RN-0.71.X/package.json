{
  "name": "scanbotsdkexample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "clean-android": "cd android && ./gradlew app:clean && cd ..",
    "clean-ios": "cd ios && rm -rf Pods/ && rm Podfile.lock && pod install --repo-update && cd ..",
    "clean-full": "npm run clean-android && npm run clean-ios && npm run reinstall",
    "reinstall": "rm -rf node_modules/ && rm package-lock.json && npm install --force",
    "postinstall": "cd ios && pod install"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "react": "18.2.0",
    "react-native": "0.71.11",
    "react-native-custom-actionsheet": "^0.1.1",
    "react-native-document-picker": "^9.0.1",
    "react-native-fs": "github:marcosaia1812/react-native-fs",
    "react-native-gesture-handler": "^2.12.0",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-safe-area-context": "^4.6.0",
    "react-native-scanbot-sdk": "4.15.3",
    "react-native-screens": "^3.24.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
